{
  "extends": "airbnb-typescript",
  "env": {
    "es6": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parserOptions": {
        "project": ["./tsconfig.json"]
      }
    }
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "linebreak-style": 0
  }
}
